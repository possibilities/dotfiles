{
  "mcpServers": {
    "kit": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "cased-kit",
        "kit-mcp"
      ]
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "augments": {
      "command": "augments-mcp-server",
      "env": {
        "AUGMENTS_CACHE_DIR": "/home/mike/.cache/augments-mcp-server"
      }
    }
  }
}
